{
  "name": "github-page",
  "homepage": "https://paterjason.github.io/",
  "scripts": {
    "shadow:watch": "shadow-cljs watch app --debug",
    "shadow:release": "shadow-cljs release app",
    "css:watch": "sass sass/mystyles.scss out/css/mystyles.css --watch",
    "css:release": "sass sass/mystyles.scss out/css/mystyles.css --no-source-map --style=compressed",
    "watch": "run-p -l *:watch",
    "release": "run-s *:release"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.0"
  },
  "devDependencies": {
    "bulma": "^0.9.3",
    "sass": "^1.49.7",
    "shadow-cljs": "^2.17.1",
    "npm-run-all": "^4.1.5"
  }
}
